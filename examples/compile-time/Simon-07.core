root = f1 0

f1 x = f2 y ++ f2 (y + 1)
  where y = (x + 1) * 2
f2 x = f3 y ++ f3 (y + 1)
  where y = (x + 1) * 2
f3 x = f4 y ++ f4 (y + 1)
  where y = (x + 1) * 2
f4 x = f5 y ++ f5 (y + 1)
  where y = (x + 1) * 2
f5 x = f6 y ++ f6 (y + 1)
  where y = (x + 1) * 2
f6 x = f7 y ++ f7 (y + 1)
  where y = (x + 1) * 2
f7 x = [x + 1]

(++) xs ys = case xs of
    [] -> ys
    (z:zs) -> z : (zs ++ ys)

null xs = case xs of [] -> True; (y:ys) -> False

tests = [
    (null root, False)
  ]
